}

//TDK:: set class by position of fly cart icon
function getOffsetFlycartIcon()
{
	if ($('.tdk-fly-cart.solo .tdk-fly-cart-icon').length)
	{
		var offset_top = $('.tdk-fly-cart.solo .tdk-fly-cart-icon').offset().top;
		var offset_left = $('.tdk-fly-cart.solo .tdk-fly-cart-icon').offset().left;
		var window_width = $(window).width();
		
		if (offset_left <= window_width/2)
		{
			$('.tdk-fly-cart.solo').removeClass('offset-right').addClass('offset-left');
		}
		else
		{
			$('.tdk-fly-cart.solo').removeClass('offset-left').addClass('offset-right');
		}
		
		//TDK:: update rtl
		if (lf_is_gen_rtl && prestashop.language.is_rtl == 1)
		{			
			if ($('.tdk-fly-cart.solo').hasClass('offset-right'))
			{
				$('.tdk-fly-cart.solo').removeClass('offset-right').addClass('offset-left');
			}
			else if ($('.tdk-fly-cart.solo').hasClass('offset-left'))
			{
				$('.tdk-fly-cart.solo').removeClass('offset-left').addClass('offset-right');
			}
		}
	}
}

//TDK:: show modal popup cart
function showModalPopupCart(modal)
{
	if ($('#blockcart-modal').length)
	{
		$('#blockcart-modal').remove();
	}
	
	$('body').append(modal);
	
	$('#blockcart-modal').modal('show');	
}