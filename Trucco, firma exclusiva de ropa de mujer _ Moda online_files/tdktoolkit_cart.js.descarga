		if (e.keyCode == 27) { 
			if ($('.tdk-fly-cart-mask').hasClass('active'))
			{
				$('.tdk-fly-cart-mask.active').trigger('click');
			}
			else if ($('.tdk-fly-cart-slidebar.active .tdk-fly-cart-icon').length)
			{
				$('.tdk-fly-cart-slidebar .tdk-fly-cart-icon').trigger('click');
			}
		}
	});
}

//TDK:: update scroll bar for slidebar cart
function checkFlyCartScrollBar($element, $show_attr) {
	var object_parent = $element.parents('.tdk-fly-cart-slidebar');
	if (object_parent.hasClass('slidebar_top') || object_parent.hasClass('slidebar_bottom')) {
		// console.log('test');
		var width_bottom = object_parent.find('.tdk-dropdown-bottom').outerWidth();
		var window_width = $(window).width();		
		var element_width = '';
		if (typeof width_cart_item != 'undefined') {
			element_width = $element.find('.tdk-dropdown-cart-item').length * (parseInt(width_cart_item) + 30);
			if ($element.find('.show-additional').length || $show_attr) {
				element_width = parseInt(element_width) + parseInt(width_cart_item);
			}
		} else {
			element_width = $element.find('.tdk-dropdown-cart-item').length * ($element.find('.tdk-dropdown-cart-item').outerWidth() + 30);
			if ($element.find('.show-additional').length || $show_attr) {
				element_width = parseInt(element_width) + parseInt($element.find('.tdk-dropdown-cart-item').outerWidth());
			}
		}
		// console.log(width_bottom);	
		// console.log(element_width);
		// console.log(window_width);
		if (element_width + width_bottom > window_width - 60)
		{
			object_parent.addClass('active-scroll');
			object_parent.find('.tdk-dropdown-list-item-warpper').addClass('active-scrollbar');
			object_parent.find('.tdk-dropdown-list-item-warpper').css({'width': window_width - width_bottom - 60});
			object_parent.find('.tdk-dropdown-list-item-warpper').mCustomScrollbar({
				theme:"dark",
				axis: "x",
				scrollInertia: 200,
				callbacks:{
					onInit:function(){
					  
					}
				},
				advanced:{
					autoExpandHorizontalScroll:true
				},
				keyboard:{
					enable:true,
					// scrollType:"stepless",
					// scrollAmount:"auto"
				}
			});
			object_parent.find('.tdk-dropdown-list-item-warpper').mCustomScrollbar('update');
		}
		else
		{
			object_parent.removeClass('active-scroll');
			object_parent.find('.tdk-dropdown-list-item-warpper').removeClass("active-scrollbar").css({'width': window_width - width_bottom - 60});
			object_parent.find('.tdk-dropdown-list-item-warpper').mCustomScrollbar("destroy");
		}
	}
	
	if (object_parent.hasClass('slidebar_left') || object_parent.hasClass('slidebar_right'))
	{
		var height_bottom = object_parent.find('.tdk-dropdown-bottom').outerHeight();
		var height_header = object_parent.find('.tdk-fly-cart-header').outerHeight();
		var window_height = $(window).height();		
		var element_height = '';
		if (typeof height_cart_item != 'undefined')
		{
			element_height = $element.find('.tdk-dropdown-cart-item').length * (parseInt(height_cart_item) + 20);		
		}
		else
		{
			element_height = $element.find('.tdk-dropdown-cart-item').length * ($element.find('.tdk-dropdown-cart-item').outerHeight() + 20);
		}
		// console.log(element_height);
		if ($element.find('.show-additional').length || $show_attr) {
			element_height = parseInt(element_height) + parseInt($element.find('.show-additional .tdk-dropdown-additional').height());
		}
		// console.log(element_height);
		if (element_height + height_bottom + height_header > window_height - 40)
		{		
			object_parent.addClass('active-scroll');
			$element.addClass('active-scrollbar');
			$element.css({'max-height': window_height - height_bottom - height_header - 40});
			$element.css({'height': window_height - height_bottom - height_header - 40});
			$element.mCustomScrollbar({
				theme:"dark",
				scrollInertia: 200,
				callbacks:{
					onInit:function(){
					  
					}
				},
				keyboard:{
					enable:true,
					// scrollType:"stepless",
					// scrollAmount:"auto"
				}

			});
			$element.mCustomScrollbar('update');
		}
		else
		{		
			object_parent.removeClass('active-scroll');
			$element.removeClass("active-scrollbar").css({'max-height': 'none'});
			$element.css({'height': window_height - height_bottom - height_header - 40});
			$element.mCustomScrollbar("destroy");
		}
	}
	
}

