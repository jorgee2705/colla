//TDK:: event for button add cart
function tdkBtCart()
{
	$('.tdk-bt-cart').each(function(){
		if (!$(this).hasClass('tdk-enable'))
		{	
			$(this).addClass('tdk-enable');
			$(this).click(function(event){
				if ($(this).hasClass('active') || $(this).hasClass('reset') || $('.tdk-bt-cart.active').length || $(this).hasClass('disabled'))
				{
					return false;
				}
				
				$(this).find('.tdk-bt-cart-content').hide();
				$(this).find('.tdk-loading').css({'display':'block'});
				$(this).addClass('active');
				// console.log('aaa');
				// return false;
				// var object_button_container = $(this).parents('.button-container');
				//TDK:: at product list
				var object_button_container = $(this).parents('.product-miniature');
				
				//TDK:: at product page and quickview
				if ($(this).parents('.product-actions').length)
				{
					object_button_container = $(this).parents('.product-actions');
				}
				
				if (object_button_container.find('.tdk_cart_quantity').length)
				{
					object_button_container.find('.qty_product').val(object_button_container.find('.tdk_cart_quantity').val());
				}
				
				var qty_product = object_button_container.find('.qty_product').val();
				var min_qty = object_button_container.find('.minimal_quantity').val();
				var quantity_product = object_button_container.find('.quantity_product').val();
				// console.log(qty_product);
				// console.log(min_qty);
				// console.log(quantity_product);
				if(Math.floor(qty_product) == qty_product && $.isNumeric(qty_product) && qty_product > 0)
				{
					// return true;
				}
				else
				{
					$(this).addClass('reset');
					// $(this).siblings('.qty_product').val(min_qty);
					$('.tdk-modal-cart .modal-header').addClass('warning-mess');
					$('.tdk-modal-cart .tdk-warning').show();
					$('.tdk-modal-cart').modal('show');
					return false;
				}
				// $('.tdk-modal-cart .modal-header').addClass('block-mess');
				// $('.tdk-modal-cart .tdk-block').show();
				// $('.tdk-modal-cart').modal('show');
				if (parseInt(qty_product) < parseInt(min_qty))
				{
					$(this).addClass('reset');
					$('.tdk-modal-cart .modal-header').addClass('info-mess');
					$('.tdk-modal-cart .tdk-info .alert-min-qty').text(min_qty);
					$('.tdk-modal-cart .tdk-info').show();
					$('.tdk-modal-cart').modal('show');
					return false;
				}
				
				//TDK:: check product out stock
				var id_product = object_button_container.find('.id_product').val();
				var id_product_attribute = object_button_container.find('.id_product_attribute').val();
				var id_customization = object_button_container.find('.id_customization').val();
				// var check_product_outstock = checkProductOutStock(id_product, id_product_attribute, qty_product);
				// console.log(check_product_outstock);
				// if (parseInt(qty_product) > parseInt(quantity_product))
				// {
					// $(this).addClass('reset');
					// $('.tdk-modal-cart .modal-header').addClass('block-mess');			
					// $('.tdk-modal-cart .tdk-block').show();
					// $('.tdk-modal-cart').modal('show');
					// return false;
				// }
				// return false;
				// console.log(check_product_outstock);
				var $element = $(this);
				$(this).removeData('check-outstock');
				
				// $.when(checkProductOutStock(id_product, id_product_attribute, id_customization, qty_product, $element, true)).done(function(data){
					checkProductOutStock(id_product, id_product_attribute, id_customization, qty_product, $element, true);
					var check_data_outstock = setInterval(function(){					
						if (typeof $element.data('check-outstock') != 'undefined')
						{							
							clearInterval(check_data_outstock);
							if (!$element.data('check-outstock'))
							{
								// console.log('bbbb');
								$element.addClass('reset');
								$('.tdk-modal-cart .modal-header').addClass('block-mess');			
								$('.tdk-modal-cart .tdk-block').show();
								$('.tdk-modal-cart').modal('show');
								// return false;
								event.preventDefault();
								event.stopPropagation();
								// check_product_outstock = false;
							}
							else
							{
								var $form = $element.closest('form');
								var query = $form.serialize() + '&add=1&action=update';
								var actionURL = $form.attr('action');
								
								//TDK:: run add to cart
								$.ajax({
									type: 'POST',
									headers: {"cache-control": "no-cache"},
									url: actionURL,
									async: true,
									cache: false,
									data: query,
									dataType: 'json',
									success: function (result)
									{
										if(result.success)
										{							
											if ($('.tdk-notification').length && typeof enable_notification != 'undefined' && enable_notification)
											{
												//TDK:: show notification
												var id_product = result.id_product;
												var product_name = false;
												$.each(result.cart.products, function(key, value){
													// console.log(value);
													// console.log(value.id_product);			
													// console.log(value.name);
													if (id_product == value.id_product)
													{
														product_name = value.name;
														return false;
													}
												})				
												showTdkNotification('success', 'add', product_name);				
											}
											
											//TDK:: run fly cart
											if (typeof enable_flycart_effect != 'undefined' && enable_flycart_effect) {
												if ($('.tdk-bt-cart.active').length) {
													//TDK:: product list
													flyCartEffect($('.tdk-bt-cart.active'));
												}
											}
											
											if (typeof show_sidebarcart != 'undefined' && show_sidebarcart && $('.tdk-fly-cart-slidebar').length) {
												showSidebarCartAfterAddCart();
											}
											
											//TDK:: hide popup quickview if click on quickview
											if($('.quickview.modal').length) {
												$('.quickview.modal').modal('hide');
											}
											
											//TDK:: loading fly cart
											if ($('.tdk-fly-cart .tdk-fly-cart-cssload-loader').length) {
												$('.tdk-fly-cart .tdk-fly-cart-cssload-loader').show();
											}
											
											//TDK:: loading default cart
											if ($('.tdk-blockcart.cart-preview .cssload-piano').length) {
												$('.tdk-blockcart.cart-preview .cssload-piano').show();
											}
											
											//TDK:: reset button add cart
											if (typeof show_popup != 'undefined' && !show_popup) {
												if ($('.tdk-bt-cart.active').length) {
													$('.tdk-bt-cart.active').find('.tdk-bt-cart-content').fadeIn('fast');
													$('.tdk-bt-cart.active').find('.tdk-loading').hide();
													$('.tdk-bt-cart.active').removeClass('active reset');
												}
												
												//TDK:: refresh default cart
												var refresh_url = $('.tdk-blockcart').data('refresh-url');
												$.ajax({
													type: 'POST',
													headers: {"cache-control": "no-cache"},
													url: refresh_url,
													async: true,
													cache: false,										
													success: function (resp)
													{
														$('.tdk-blockcart').replaceWith($(resp.preview).find('.blockcart'));
														$(".blockcart.cart-preview").addClass('tdk-blockcart').removeClass('blockcart');
														//TDK:: refresh default cart
														createModalAndDropdown(1, 0, 0);
														
													},
													error: function (XMLHttpRequest, textStatus, errorThrown) {
														console.log("TECHNICAL ERROR: \n\nDetails:\nError thrown: " + XMLHttpRequest + "\n" + 'Text status: ' + textStatus);
													}
												});
											}
																		
											if (typeof show_popup != 'undefined' && show_popup)
											{
												var refreshURL = $('.blockcart').data('refresh-url');
												var requestData = {};
											
												requestData = {
													id_product_attribute: result.id_product_attribute,
													id_product: result.id_product,
													action: 'add-to-cart'
												};
												
												$.ajax({
													type: 'POST',
													headers: {"cache-control": "no-cache"},
													url: refreshURL,
													async: true,
													cache: false,
													data: requestData,
													dataType: 'json',
													success: function (resp)
													{
														if ($('.tdk-bt-cart.active').length)
														{
															$('.tdk-bt-cart.active').find('.tdk-bt-cart-content').fadeIn('fast');
															$('.tdk-bt-cart.active').find('.tdk-loading').hide();
															$('.tdk-bt-cart.active').removeClass('active reset');
														}
														
														//TDK:: refresh default cart
														$('.blockcart').replaceWith($(resp.preview).find('.blockcart'));
														//TDK:: show modal popup
														if (resp.modal) {													
															showModalPopupCart(resp.modal);
														}
														
														//TDK:: refresh dropdown cart
														createModalAndDropdown(1, 0, 0);	
													},
													error: function (XMLHttpRequest, textStatus, errorThrown) {
														console.log("TECHNICAL ERROR: \n\nDetails:\nError thrown: " + XMLHttpRequest + "\n" + 'Text status: ' + textStatus);
													}
												});
																
											}																
																			
										}
										else
										{
											//TDK:: show notification
											showTdkNotification('error','add', false);
										}
									},
									error: function (XMLHttpRequest, textStatus, errorThrown) {
										// alert("TECHNICAL ERROR: \n\nDetails:\nError thrown: " + XMLHttpRequest + "\n" + 'Text status: ' + textStatus);
										showTdkNotification('error','add', false);
									}
								});
							}
						}			
					}, 10);
					// console.log($element);
					// console.log($element.data('check-outstock'));
					
				// });
				
				
				return false;
			});
		};
	});
	
	$('.tdk_cart_quantity').each(function(){
		//TDK:: hide input quantity when cart buton does not show
		var obj_parent = $(this).parents('.product-miniature');
		
		//TDK:: for add cart at product page
		if ($(this).parents('.product-actions').length)
		{
			obj_parent = $(this).parents('.product-actions');
		}
		
		if (obj_parent.find('.qty_product').val())
		{			
			$(this).val(obj_parent.find('.qty_product').val());
		}
		else
		{
			$(this).hide();
		}
	});
	
	//TDK:: change quantity product dropdown with button up-down
	$('.tdk-bt-pro-qty-down, .tdk-bt-pro-qty-up').on('touchstart click', function(){
		
		// console.log('test');
		var action = 'up';
		var input_target = $(this).parents('.tdk-pro-qty').find('.tdk_cart_quantity');
		var input_qty = parseInt(input_target.val());
		// console.log(input_qty);
		var qty_update;
		if ($(this).hasClass('tdk-bt-pro-qty-down'))
		{
			action = 'down';
		}
		
		if (action == 'up')
		{
			qty_update = input_qty+1;
		}
		if (action == 'down')
		{
			qty_update = input_qty-1;
			if (qty_update < 1)
			{
				qty_update = 1;
			}
		}
		input_target.val(qty_update);
		// updateQuantityProductDropDown(input_target);
		// console.log(update_quantity);
		
		return false;
	})
	// $('.tdk_cart_quantity').change(function(){
		// console.log($(this).val());
		// $(this).parents('.product-miniature').find('.qty_product').val($(this).val());
	// })
}

