			$element.addClass('active-scrollbar').css({'max-height': $element.find('.tdk-dropdown-cart-item').outerHeight()*check_number_cartitem});
		}
		
		$element.mCustomScrollbar({
			theme:"dark",
			scrollInertia: 200,
