//TDK:: update position label
function updatePostionLabel($parent)
{
	var FLAG_MARGIN = 10;
      var $percent = $parent.find('.discount-percentage');
      var $onsale =  $parent.find('.on-sale');
      var $new = $parent.find('.new');
      if($percent.length){
        $new.css('top', $percent.height() * 2 + FLAG_MARGIN);
        $percent.css('top',-$parent.find('.thumbnail-container').height() + $parent.find('.product-description').height() + FLAG_MARGIN);
      }
      if($onsale.length){
        $percent.css('top', parseFloat($percent.css('top')) + $onsale.height() + FLAG_MARGIN);
        $new.css('top', ($percent.height() * 2 + $onsale.height()) + FLAG_MARGIN * 2);
      }
}

//TDK:: show dropdown cart
function showDropDownCart($element, $type)
{
	var object_element = '';
	if ($type == 'defaultcart')
	{
		object_element = $element.siblings('.tdk-dropdown-cart');
	}
	if ($type == 'flycart')
	{
		object_element = $element.parents('.tdk-fly-cart').find('.tdk-dropdown-cart');
	}
	
	if (!object_element.hasClass('show'))
	{
		object_element.addClass('show');
	}
	else
	{
		object_element.removeClass('show');
	}
	
}

//TDK:: show dropdown cart
function showSlideBarCart($element)
{
	if (!$('.tdk-fly-cart-slidebar.disable').length)
	{
		if (!$element.hasClass('active-slidebarcart'))
		{
			var type = $element.data('type');
			var pusheffect = $element.data('pusheffect');
			$element.addClass('active-slidebarcart');
			
			$('.tdk-fly-cart-slidebar.'+type).addClass('active');
			if ($('.tdk-fly-cart-mask').length)
			{
				$('.tdk-fly-cart-mask').first().addClass('active');
				$('body').addClass('tdkflycart-active-slidebar');
			}
			//TDK:: update scrollbar of slidebar cart for safari win
			if ($('html').hasClass('safari-win'))
			{				
				$('.tdk-fly-cart-slidebar .tdk-dropdown-list-item').each(function(){		
					//TDK:: scroll bar for slidebar
					checkFlyCartScrollBar($(this), 0);
				})
			};

			if (pusheffect)
			{
				$('.tdk-fly-cart-slidebar.'+type).addClass('push');
				$('body').addClass('tdkflycart-active-push');
				var push_value;
				var push_type;
				if (type == 'slidebar_top' || type == 'slidebar_bottom')
				{
					push_type = "Y";
					if (type == 'slidebar_top')
					{
						// push_value = $('.tdk-fly-cart-slidebar.push.'+type).outerHeight()+$('.tdk-fly-cart-slidebar.push.'+type+' .tdk-fly-cart').outerHeight();
						push_value = $('.tdk-fly-cart-slidebar.push.'+type).outerHeight();
					}
					if (type == 'slidebar_bottom')
					{
						// push_value = -($('.tdk-fly-cart-slidebar.push.'+type).outerHeight()+$('.tdk-fly-cart-slidebar.push.'+type+' .tdk-fly-cart').outerHeight());
						push_value = -$('.tdk-fly-cart-slidebar.push.'+type).outerHeight();
					}

				}
				if (type == 'slidebar_right' || type == 'slidebar_left')
				{
					push_type = "X";
					if (type == 'slidebar_left')
					{
						// push_value = $('.tdk-fly-cart-slidebar.push.'+type).outerWidth()+$('.tdk-fly-cart-slidebar.push.'+type+' .tdk-fly-cart').outerWidth();
						push_value = $('.tdk-fly-cart-slidebar.push.'+type).outerWidth();
					}
					if (type == 'slidebar_right')
					{
						// push_value = -($('.tdk-fly-cart-slidebar.push.'+type).outerWidth()+$('.tdk-fly-cart-slidebar.push.'+type+' .tdk-fly-cart').outerWidth());
						push_value = -$('.tdk-fly-cart-slidebar.push.'+type).outerWidth();
					}

				}
				$('body.tdkflycart-active-push main').css({
					"-moz-transform": "translate"+push_type+"("+push_value+"px)",
					"-webkit-transform": "translate"+push_type+"("+push_value+"px)",
					"-o-transform": "translate"+push_type+"("+push_value+"px)",
					"-ms-transform": "translate"+push_type+"("+push_value+"px)",
					"transform": "translate"+push_type+"("+push_value+"px)",
				})
			}
			
		}
		else
		{
			$('.tdk-fly-cart-slidebar .tdk-fly-cart-icon').trigger('click');
		}
	}
}

