//TDK:: create fly cart effect
function flyCartEffect($element) {
	//TDK:: at product list
	if ($element.hasClass('tdk-bt-cart') && $element.parents('.product-miniature').length)
	{
		var parent_e = $element.parents('.product-miniature');
		var product_img = parent_e.find('.product-thumbnail').find("img").eq(0);
	}
	else
	{
		//TDK: at product page and quickview
		var product_img = '';
		if ($('.quickview .product-cover').length)
		{
			//TDK:: quickview in produc page
			product_img = $('.quickview .product-cover').find("img").eq(0);
		}
		else
		{
			//TDK:: product page
			product_img = $('.product-cover').find("img").eq(0);
		}	
	}
	if (typeof product_img != 'undefined' && product_img.length)
	{		
		var obj_element = '';
		// if ($('.tdk-fly-cart-slidebar.active .tdk-fly-cart-icon').length)
		// {
			// obj_element = $('.tdk-fly-cart-slidebar.active .tdk-fly-cart-icon');
